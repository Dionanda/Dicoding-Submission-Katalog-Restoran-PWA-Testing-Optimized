{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-undef": "off",
        "consistent-return": "off",
        "no-prototype-builtins": "off",
        "import/prefer-default-export": "off",
        "eqeqeq": "off",
        "no-return-assign": "off"
    }
}
